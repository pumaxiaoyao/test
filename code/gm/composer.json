{
    "name": "gm",
    "description": "The gm site.",
    "keywords": ["gm", "site"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "filp/whoops": "*",
        "gregwar/captcha": "^1.1",
        "illuminate/database": "^5.6",
        "xiaoler/blade": "^5.4"
    },
    "require-dev": {
    },
    "autoload": {
        "file": [
            "app/Libs/GeetestLib.php",
            "app/Libs/HttpRequest.php",
            "app/Libs/HttpResponse.php",
            "app/Controllers/Def.php",
            "app/Config/Config.php"
            
        ],
        "classmap": [
            "app/Controllers",
            "app/ViewHelper",
            "app/Models",
            "app/Core"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
